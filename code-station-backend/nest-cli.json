{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "generateOptions": {
    "spec": false
  },
  "sourceRoot": "apps/code-station-backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true,
    "assets": [
      "**/*.env*"
    ],
    "webpack": true,
    "tsConfigPath": "apps/code-station-backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/code-station-backend",
  "projects": {
    "code-station-backend": {
      "type": "application",
      "root": "apps/code-station-backend",
      "entryFile": "main",
      "sourceRoot": "apps/code-station-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/code-station-backend/tsconfig.app.json"
      }
    },
    "chat-room-backend": {
      "type": "application",
      "root": "apps/chat-room-backend",
      "entryFile": "main",
      "sourceRoot": "apps/chat-room-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat-room-backend/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "minio": {
      "type": "library",
      "root": "libs/minio",
      "entryFile": "index",
      "sourceRoot": "libs/minio/src",
      "compilerOptions": {
        "tsConfigPath": "libs/minio/tsconfig.lib.json"
      }
    },
    "winston": {
      "type": "library",
      "root": "libs/winston",
      "entryFile": "index",
      "sourceRoot": "libs/winston/src",
      "compilerOptions": {
        "tsConfigPath": "libs/winston/tsconfig.lib.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    }
  }
}